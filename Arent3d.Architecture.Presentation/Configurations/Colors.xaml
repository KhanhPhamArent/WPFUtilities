    <SolidColorBrush x:Key="GlyphLightColorBrush">White</SolidColorBrush>
    <SolidColorBrush x:Key="GlyphMediumColorBrush">#FFA3A3A3</SolidColorBrush>
    <SolidColorBrush x:Key="GlyphDarkColorBrush">#FF444444</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundDarkColorBrush">Black</SolidColorBrush>
    <!--Border colors-->
    <SolidColorBrush x:Key="BorderLightColorBrush">White</SolidColorBrush>
    <SolidColorBrush x:Key="BorderMediumColorBrush">#FFA3A3A3</SolidColorBrush>
    <SolidColorBrush x:Key="BorderDarkColorBrush">#FF009BE0</SolidColorBrush>

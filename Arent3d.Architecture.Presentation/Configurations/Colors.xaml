    <SolidColorBrush x:Key="ControlLightColorBrush">White</SolidColorBrush>
    <SolidColorBrush x:Key="ControlMediumColorBrush">#FFE5E5E5</SolidColorBrush>
    <SolidColorBrush x:Key="ControlDarkColorBrush">#FF009BE0</SolidColorBrush>
